<div class="container">
  <h1>cart</h1>
  {{#each cartItems as |cartItem|}}
    <div class="">
      <div class="">
        name: {{cartItem.name}}
      </div>
      <div class="">
        quantity: {{cartItem.quantity}}
      </div>
    </div>
  {{else}}
    <div class="card bg-faded">
      empty
    </div>
  {{/each}}
</div>
